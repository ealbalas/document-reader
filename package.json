{
  "name": "pdf-qa-app",
  "version": "1.0.0",
  "description": "PDF Document Q&A Application with React frontend",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm start\" \"cd backend && python app.py\"",
    "build:worker": "esbuild src/worker.js --bundle --format=esm --outfile=dist/worker.js --platform=neutral --target=es2022",
    "dev:worker": "wrangler dev",
    "deploy:worker": "wrangler deploy",
    "preview:worker": "wrangler preview",
    "install:worker": "npm install itty-router pdf-lib esbuild wrangler --save-dev"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^7.5.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "esbuild": "^0.25.8",
    "itty-router": "^5.0.21",
    "pdf-lib": "^1.17.1",
    "wrangler": "^4.28.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

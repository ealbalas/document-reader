# PDF Reader Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Required: At least one AI provider API key
OPENAI_API_KEY=openai_key
GEMINI_API_KEY=google_key

# Optional: LLM Configuration
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o
LLM_TEMPERATURE=0.1
LLM_MAX_TOKENS=1000

# Optional: Embedding Configuration
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=text-embedding-large

# Optional: Flask Configuration
FLASK_ENV=development
PORT=5002

# Optional: Advanced Configuration
# LLM_PROVIDER options: openai, gemini
# LLM_MODEL options (OpenAI): gpt-4o, gpt-4o-mini, gpt-4, gpt-4-turbo, gpt-3.5-turbo
# LLM_MODEL options (Gemini): gemini-1.5-flash, gemini-1.5-pro, gemini-pro
# EMBEDDING_PROVIDER options: sentence-transformers, openai
# EMBEDDING_MODEL options (sentence-transformers): all-mpnet-base-v2, all-MiniLM-L6-v2, multi-qa-MiniLM-L6-cos-v1
# EMBEDDING_MODEL options (OpenAI): text-embedding-ada-002, text-embedding-3-small, text-embedding-3-large
